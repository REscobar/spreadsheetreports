<Window x:Class="SpreadSheetsReports.WpfUi.Cells.CellBindingsEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpreadSheetsReports.WpfUi.Cells"
        mc:Ignorable="d"
        Title="CellBindingsEditor" Height="190" Width="300"
         d:DataContext="{d:DesignInstance Type=local:CellBinder, IsDesignTimeCreatable=True}" >
    <Grid Margin="0,0,0,0">
        <local:CellBindingEditor Margin="10" x:Name="Editor"/>
        <Button Content="Ok" Margin="0,0,96,10" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="81" Click="Ok_Click" />
        <Button x:Name="button" Content="Cancel" Margin="0,0,10,10" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="81" Click="Cancel_Click"/>
    </Grid>
</Window>
