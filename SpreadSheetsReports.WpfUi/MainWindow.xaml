<Window x:Class="SpreadSheetsReports.WpfUi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpreadSheetsReports.WpfUi"
        xmlns:document="clr-namespace:SpreadSheetsReports.WpfUi.Documents"
        mc:Ignorable="d"
        Title="SpreadSheets Report Designer" Height="450" Width="525" Icon="icon.ico">
    <!--<Window.DataContext>
        <cells:CellBinder/>
    </Window.DataContext>-->
    <DockPanel>
        <ToolBarTray DockPanel.Dock="Top">
            <ToolBar>
                <Button Content="New" Click="New_Click"></Button>
                <Button Content="Open" Click="Open_Click"></Button>
                <Button Content="Save" Click="Save_Click"></Button>
            </ToolBar>
        </ToolBarTray>
        <document:DocumentEditor x:Name="Editor"/>

    </DockPanel>
</Window>
